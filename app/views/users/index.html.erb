<% provide(:title, 'ユーザー一覧') %>

<div class="container">
  <div class="col-sm-8 offset-sm-2">
    <h1>すべてのユーザー</h1>
    <%= will_paginate %>
    <ul class="users list-group">
      <% @users.each do |user| %>
        <%= render user %>
      <% end %>
    </ul>
    <%= will_paginate %>
  </div>
</div>
